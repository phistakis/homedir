function ren() { for f in `find . -name '*$1'`; do mv $f `echo $f | sed -e 's/$1/$2/g'` ; done; }

alias class=def
alias venv='. /intucell/latest/bin/activate'
alias vtenv='. /intucell/latest/testing/bin/activate'
alias title=label
alias grepi='\grep --color'
