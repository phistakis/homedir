function gbc () { git branch | grep $1 | xargs git checkout ; }
function gload() { gf ; g stash ; g reset --hard origin/avivshahar/`git symbolic-ref HEAD | sed -e 's,.*/\(.*\),\1,'` ; g stash pop ; }
function gbs () { git branch | grep $1 | xargs git show ; }
function gbss () { git branch | grep $1 | xargs git show --stat; }
function gbd () { git branch | grep $1 | head -n 1 | xargs git branch -d ; }
function gbD () { git branch | grep $1 | head -n 1 | xargs git branch -D ; }
function gpopto () { git stash && export LAST_BRANCH=`git rev-parse --abbrev-ref HEAD` && gbc $1; }
function gpopback () { gbc $LAST_BRANCH && g stash pop; }
function gprps () { git push -f origin HEAD:$1/`git symbolic-ref HEAD | sed -e 's,.*/\(.*\),\1,'`/$USER; }

